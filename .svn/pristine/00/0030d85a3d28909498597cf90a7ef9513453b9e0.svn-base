					<!-- TABSET -->
					<div class="row">
						<tabset class="canopi-tabset" justified="false"> 
							<tab heading="Regular UNI Sites" ng-click="$broadcast('layout')">
								<div class="row">
									<!-- <div class="row" style="padding-left: 20px; padding-right: 20px;"> -->
										<div class="col-md-12">
											<button href='javascript:void(0);' class="btn canopi-btn-blue"><span>Add Site</span></button>
											<button href='javascript:void(0);' class="btn canopi-btn-gray"><span>Delete Selected Sites</span></button>
										</div>
										<!-- DATATABLE Regular Uni Sites -->
										<div class="table-responsive col-lg-12 no-padding" style="padding-top: 10px;">						
											<table class="table table-striped table-hover data-table"
												search-result-table id="firstTable"
												table-data="resultsets['RegularUNI'].resultData" add-export="true"
												event-handler="gridEventHandler(eventId, data)">
											</table>					
										</div>
									<!-- </div> -->
								</div>								
							</tab>							
							<tab heading="MPA Shared UNI Sites" ng-click="$broadcast('layout')">
								<div class="row">

                                    <div class="col-md-12">
										<button href='javascript:void(0);' class="btn canopi-btn-blue"><span>Add Site</span></button>
										<button href='javascript:void(0);' class="btn canopi-btn-gray"><span>Delete Selected Sites</span></button>
									</div>

                                    <!-- DATATABLE Shared Uni Sites -->
									<div class="table-responsive col-lg-12 no-padding" style="padding-top: 10px;">						
										<table class="table table-striped table-hover data-table"
											search-result-table id="secondTable"
											table-data="resultsets['MPASharedUNI'].resultData" add-export="true"
											event-handler="gridEventHandler(eventId, data)">
										</table>
									</div>

								</div>
							</tab> <!-- End external order info -->
						</tabset><!-- End tabset -->
					</div><!-- End row tabset -->
					
										
					<div class="row">
						<tabset class="canopi-tabset" justified="false"> 
							<tab heading="Critical Dates">
