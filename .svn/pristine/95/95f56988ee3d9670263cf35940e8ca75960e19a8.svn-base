/* Landscape Phones / Portrait tablet */
/* Boostrap .col-xs-* */
@media ( min-width : 481px) and (max-width: 767px) {

    .canopi-site-footer ul li {
        display: inline;
        font-size: 9px;
        padding: 7px;
    }
    .canopi-site-footer .canopi-footer-logo {
        text-align: center;
        padding: 0;
    }
    .canopi-site-footer .canopi-footer-allRightsReserved {
        font-size: 11px;
        padding-top: 5px;
        padding-bottom: 5px;
    }
    .canopi-site-footer .canopi-footer-links {
        text-align: center;
        padding: 0;
    }

}

/* Landscape Phones and down */
/* Boostrap .col-xs-* */
@media ( max-width : 480px) {

    #aboutModalDialog {
        padding-left: 35px;
        padding-right: 35px;
    }
    #canopi-nav-collapsed {
        margin-left: 10px;
    }
    .canopi-site-footer .canopi-footer-logo {
        text-align: center;
        padding: 0;
    }
    .canopi-site-footer .canopi-footer-allRightsReserved {
        font-size: 11px;
        padding-top: 5px;
        padding-bottom: 5px;
    }
    .canopi-site-footer .canopi-footer-links {
        text-align: center;
        padding: 0;
    }

}

