	padding: 10px;
	margin: 0;
}


/* Will be 12px otherwise for "Search Results:" */
div.dataTables_filter label {
	font-size: 14px;
}

/* Need this to force search field in table to be slightly smaller than large input fields in forms
   and to flow from left to right */
div.dataTables_filter .form-control {
	display: inline-block;
	height: 30px;
}

table.dataTable td {
	white-space: nowrap;
}

/* Used in datatables sort icon alignment */
.css_right {
	float: right;
}

/* Turn off the small sort icon that shows up to the right and below (in almost all cases) the header.
   This only effects the span injected after the column header text, not the outter sort style */
.DataTables_sort_icon {
	display: none;
}

table.dataTable td.dataTables_empty {
	color: red;
}

.dataTables_paginate span .ui-state-disabled,.pagination.alternate li.active a
	{
	color: #ffffff !important;
	cursor: default !important;
	-webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2) inset !important;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2) inset !important;
	opacity: 1;
	filter: alpha(opacity = 100);
	/* Highlights the active page */
	border: 1px solid #333 !important;
}

div.dataTables_paginate {
	padding: 10px 2px 10px 2px;
}



