/***********************************************
    6. Panels
************************************************/
.canopi-panel {
	border-color: #bbb;
	border-radius: 0;
}
.canopi-panel .panel-heading {
	color: #fff;
	background-color: #777;
	border-color: #777;
	border-radius: 0;
}
.canopi-panel1 {
	height: 205px;
	border-color: #bbb;
	border-radius: 0;
}
.canopi-panel2 {
	height: 890px;
	border-color: #bbb;
	border-radius: 0;
}
.canopi-panel1 .panel-heading {
	color: #fff;
	background-color: #777;
	border-color: #777;
	border-radius: 0;
	height: 45px;
}
.canopi-panel2 .panel-heading {
	color: #fff;
	background-color: #777;
	border-color: #777;
	border-radius: 0;
}
.canopi-panel1 .panel-title {
	padding-top: 5px;
}
.canopi-panel2 .panel-title {
	padding-top: 5px;
}
.canopi-panel-text li {
	padding-top: 5px;
	border-bottom: 1px solid #dddddd;
	font-size: 12px;
	list-style-type: none;
}
.canopi-panel-text li a {
	color: #ef6f00;
}
.canopi-panel-heading {
	color: #fff;
	background-color: #777;
	border-color: #777;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	padding: 10px 15px;
}



/***********************************************
    7. Modals
************************************************/
.canopi-modal-content {
	-webkit-box-shadow: 0 0px 0px rgba(0, 0, 0, 0);
	box-shadow: 0 0px 0px rgba(0, 0, 0, 0);
    min-width: 550px;
}
.canopi-modal-content .modal-header {
	background-color: #ffffff;
    padding-bottom: 0px;
}
.canopi-modal-content .modal-header .modal-title {
    color: #ef6f00;
    font-size: 26px;
}
.canopi-modal-content .modal-footer {
	background-color: #bbb;
}



/***********************************************
    8. Forms
************************************************/
.form-control {
  display: block;
  width: 100%;
  height: 34px;
  padding: 6px 12px;
  font-size: 14px;
  line-height: 1.42857143;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #ccc;
  border-radius: 0px;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
          box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
       -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
          transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.canopi-inputfields .canopi-input-title {
    color: #696969;
    font-size: 14px;
    /*font-family: Omnes-ATT-II-Medium;*/
}
.required .canopi-input-title:after{
	content:"*";
	color:red;
	font-size: 20px;
	font-size: 20px;
	line-height:10px;
    vertical-align:bottom;
}



/***********************************************
    9. Breadcrumb
************************************************/
.breadcrumb {
	padding: 0px;
	margin-bottom: 0px;
	list-style: none;
	background-color: transparent;
	border-radius: 4px;
}
.breadcrumb li a{
	color: #ef6f00;
    /*font-family: Omnes-ATT-II-Medium;*/
    font-size: 14px;
}



/***********************************************
    10. Tabs
************************************************/
.canopi-nav-tabs > li > a {
    border-top: 1px solid #B5B4B4;
    border-left: 1px solid #B5B4B4;
    border-right: 1px solid #B5B4B4;
    border-bottom: none;
    border-radius: 0px;
    color: #444;
    background-color: #f2f2f2;
    margin-bottom: -1px;
    margin-right: 0px;
}
.canopi-nav-tabs >li.active >a,
.canopi-nav-tabs >li.active >a:hover,
.canopi-nav-tabs >li.active >a:focus {
    border-top-width: 2px;
    border-top-color: #ef6f00;
    border-radius: 0px;
    background-color: #fff;
}
.canopi-nav-tabs > li > a:hover {
    border-color: rgb(192, 188, 188);
}
.canopi-tab-content {
    border: solid;
    border-width: 1px;
    border-top: none;
    border-color: #ddd;
    padding: 15px;
    background-color: #f2f2f2;
}

/*Added 10/27/14*/
.canopi-tabset{
	margin-left: 35px;
	margin-right: 35px;
	margin-bottom: 10px;
}

.canopi-tabset > .tab-content {
    border: solid;
    border-width: 1px;
    border-top: none;
    border-color: #ddd;
    padding: 15px;
    background-color: #f2f2f2;
}
.canopi-tabset > .nav-tabs > li > a {
    border-top: 1px solid #dddddd;
    border-left: 1px solid #dddddd;
    border-right: 1px solid #dddddd;
    border-bottom: none;
    border-radius: 0px;
    color: #444;
    background-color: #f2f2f2;
    margin-right: 0px;
}
.canopi-tabset > .nav-tabs >li.active >a,
.canopi-tabset > .nav-tabs >li.active >a:hover,
.canopi-tabset > .nav-tabs >li.active >a:focus {
    border-top-width: 2px;
    border-top-color: #ef6f00;
    border-radius: 0px;
    background-color: #fff;
}
#canopi-userManagement-tab {
    height: 450px;
}



/***********************************************
    11. Accordion
************************************************/
.canopi-accordion > .panel-group > .panel > .panel-heading {
	background: white;
	background-color: white;
	height: 60px;
	padding-left: 20px;
	cursor: pointer;
}
.canopi-accordion .panel-title {
	padding-top: 10px;
	padding-left: 10px;
	padding-right: 10px;
}
.canopi-accordion > .panel-group > .panel {
	border: none;
}
.canopi-accordion > .panel-group > .panel {
	border: none;
	box-shadow: 0px 1px 3px rgba(0, 0, 0, .18);
}
.canopi-accordion .panel-default > .panel-heading + .panel-collapse > .panel-body {
    border: 0;
    padding-top: 0px;
}



/***********************************************
    12. Topology
************************************************/
/* Topology container is 200px (size of menu-container) plus 3 times 250px + 2px per border*/
#topologyContainer {
    border: 2px solid gray;
    width: 958px;
    height: 500px;
}
.topologyTitle {
    padding: 10px;
    cursor: move;
    font-weight: bold;
    font-size: 120%;
}
.topologyConnect {
    width: 100%;
    height: 20px;
    cursor: pointer;
    background-color: #40B3DF;
}
.topologyItem {
    position: absolute;
    border: 1px solid black;
    width: 150px;
    font-size: 80%;
    background-color:#FFFFFF;
    text-align: center;
    -moz-box-shadow: 2px 2px 3px 1px #AAA;
    -webkit-box-shadow: 2px 2px 3px 1px #AAA;
    box-shadow: 2px 2px 3px 1px #AAA;
}



/***********************************************
    13. DataTables
************************************************/
.dataTables_scrollBody {
    max-height: 250px;
}
table.dataTable th {
    white-space: nowrap;
    text-align: left;
}

.table-responsive .dataTables_scrollHeadInner {
    padding-left: 0 !important;
}
div.dataTables_length .form-control {
    display: inline-block;
    height: 30px;
    padding-left: 8px;
    /* had some trouble when '100' was displayed on FF on some screen resolutions, ticket 16331 */
}
/* floats to left side unless we override */
.dataTables_wrapper .dataTables_length {
    float: right;
    position: relative;
    height: 30px;
    padding: 10px;
    padding-top: 15px;
    margin: 0;
}
div.DTTT.btn-group {
    padding-top: 12px;
    float: right;
}
div.dataTables_info {
    padding-top: 15px;
    padding-left: 10px;
    color: #ef6f00;
    font-size: 15px;
    /*font-family: Omnes-ATT-II-Medium;*/
}
div.ColVis {
    float: right;
    margin-bottom: 1em;
    padding-left: 10px;
}

table.dataTable thead .sorting {
    background: none;
}

table.dataTable thead .sorting_asc {
    background: url('../images/up_arrow.png') no-repeat center right;
}
table.dataTable thead .sorting_desc {
    background: url('../images/down_arrow.png') no-repeat center right;
}

table.dataTable thead > tr > th {
    padding-left: 18px;
    padding-right: 18px;
    border-right: 1px solid #d4d4d4;
    border-left: 1px solid #d4d4d4;
}

/***********************************************
    13. NG-Grid
************************************************/
.ngHeaderCell {
    border-right: 1px solid #d4d4d4;
    border-left: 1px solid #d4d4d4;
    position: absolute;
    top: 0;
    bottom: 0;
    background-color: #067AB4;
    color: white;
}

/***********************************************
    14. Dashboard
************************************************/
.canopi-dashboard-top {
	padding-top: 20px;
}
.canopi-dashboard-top .panel {
    height: 200px;
}
.canopi-dashboard-bottom .panel{
    height: 890px;
}

.shortcut-buttons {
    width: 100%;
    text-align: center;
}
.shortcut-buttons .shortcut-btn {
    margin: 15px 8px 15px;
    width: 120px;
    display: inline-block;
    border-radius: 2px;
    background: #ffffff;
    border-top: 2px solid #777;
    position: relative;
    -webkit-transition: border-color;
    transition: border-color;
    -webkit-transition-duration: 0.4s;
    transition-duration: 0.4s;
    box-shadow: 0 1px 1px rgba(0,0,0,0.3);
}
.shortcut-buttons .shortcut-btn .shortcut-icon {
    width: 100%;
    height: 60px;
    display: inline-block;
}
.shortcut-buttons .shortcut-btn .shortcut-icon i {
    padding-top: 20px;
    color: #777;
    -webkit-transition: color;
    transition: color;
    -webkit-transition-duration: 0.4s;
    transition-duration: 0.4s;
}
.shortcut-buttons .shortcut-btn .shortcut-text {
    display: inline-block;
    width: 100%;
    font-size: 11px;
    font-weight: bold;
    color: #ffffff;
    background-color: #777;
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
    -webkit-transition: all;
    transition: all;
    -webkit-transition-duration: 0.4s;
    transition-duration: 0.4s;
    height: 30px;
    padding-top: 10px;
}
.shortcut-buttons .shortcut-btn:hover {
    border-top-color: #444;
}
.shortcut-buttons .shortcut-btn:hover .shortcut-icon i {
    color: #444;
}
.shortcut-buttons .shortcut-btn:hover .shortcut-text {
    background-color: #444;
    color: #ffffff;
}

