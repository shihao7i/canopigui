/***********************************************
    1. Body
************************************************/
body {
    background-color: #f6f6f6;
    min-width: 450px;
}

/***********************************************
    2. Top Menu Navigation
************************************************/
.canopi-top {
	height: 30px;
	padding-left: 20px;
	padding-right: 20px;
}
.canopi-top > span  {
    padding-top: 5px;
}
.canopi-top .canopi-region {
    font-size: 14px;
    color: black;
}
.canopi-top .canopi-user {
	font-size: 14px;
	color: black;
}



/***********************************************
    3. Main Navigation
************************************************/

/* Brand - AT&T Logo and CANOPI version */
.canopi-brand {
	padding-top: 12px; 
	padding-right: 0px; 
	cursor: pointer;
}
.canopi-info {
	display: inline-block; 
	margin-top: 12px; 
	padding-right: 25px;
}
.canopi-info .canopi-separator {
	border-right: solid; 
	border-width: 1px; 
	line-height: 1.4; 
	font-size: 18px; 
	margin-right: 5px;
	margin-left: 5px;
	color: #fff;
}
.canopi-info .canopi-version {
	font-size: 16px; 
	line-height: 20px; 
	color: #fff;
}

/* Menu items */
.canopi-main-navigation {
	background-color: #ef6f00;
	border-color: #ef6f00;
	border-width: 0px;
	color: #ffffff;
	border-radius: 0;
}
.navbar-default .navbar-nav > li > a {
	color: #fff;
}
.navbar-default .navbar-nav > .open > a,
.navbar-default .navbar-nav > .open > a:hover,
.navbar-default .navbar-nav > .open > a:focus {
	color: #fff;
	background-color: #444;
}
.navbar-default .navbar-nav > li > a:hover,
.navbar-default .navbar-nav > li > a:focus {
	color: #fff;
	background-color: #444;
}

/* Dropdown selection menu */
.dropdown-large {
	position: static !important;
}
.dropdown-menu-large {
	padding: 10px 0px; 
	width: auto; 
	left: auto;
}
.dropdown-menu-large > li > ul > li {
	list-style: none;
}
.dropdown-menu-large > li > ul > li > a {
	display: block;
	padding: 3px 20px;
	color: #333333;
}
.dropdown-menu-large > li ul > li > a:hover,
.dropdown-menu-large > li ul > li > a:focus {
	text-decoration: none;
	color: #262626;
	background-color: #f5f5f5;
}
.dropdown-menu-large .dropdown-header {
	color: #ef6f00;
	font-size: 18px;
}
.dropdown-menu-large .col-sm-3,
.dropdown-menu-large .col-sm-6,
.dropdown-menu-large .col-sm-12 {
	padding-right: 10px;
	padding-left: 10px;
}

@media ( max-width : 991px) {
	.canopi-info {
		padding-right: 0px;
	}	
}
@media ( min-width : 768px) and (max-width: 1199px) {
	.dropdown-menu-large {
		margin-left: 0;
		margin-right: 0;
		right: 0;
	}
}
@media (max-width: 912px) {
		
	.dropdown-menu-large > li {
		margin-bottom: 30px;
	}
	
	.dropdown-menu-large > li:last-child {
		margin-bottom: 0;
	}
	
	.dropdown-menu-large .dropdown-header {
		padding: 3px 15px !important;
	}
	
	.navbar-nav .open .dropdown-menu .dropdown-header {
		color: #ef6f00;
	}
	
	.navbar-nav {
		margin: 0px -15px;
	}
	.navbar-default .navbar-nav>li>a {
		color: #ef6f00;
	}
	.navbar-default .navbar-collapse,.navbar-default .navbar-form {
		border-color: #fff;
		background-color: #fff;
	}
	
	.navbar-default .navbar-toggle {
		border-color: #ef6f00;
		background-color: #ef6f00;
	}

	.navbar-default .navbar-toggle .icon-bar {
		background-color: #fff;
	}
	
	.navbar-header {
		background-color: #ef6f00;
	}
	.navbar-default .navbar-nav>li>a {
		color: #ef6f00;
		border-bottom: solid;
		border-bottom-color: #ef6f00;
		border-bottom-width: 1px;
	}
	.navbar-default .navbar-nav>li>a:hover,.navbar-default .navbar-nav>li>a:focus
		{
		color: #ef6f00;
		border-bottom: solid;
		border-bottom-color: #ef6f00;
		background-color: transparent;
	}
	
	.navbar-default .navbar-nav > .open > a,
	.navbar-default .navbar-nav > .open > a:hover,
	.navbar-default .navbar-nav > .open >a:focus {
		background-color: #777;
	}
	
	.navbar-default .navbar-toggle:hover,
	.navbar-default .navbar-toggle:focus {
		background-color: #ff7c09;
	
}
	
	.caret {
		margin-top: 10px;
		float: right;
	}
}



