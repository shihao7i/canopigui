<div class="row">
    
    <div class="col-md-12 col-lg-12">
        
        <div class="row">
            <div class="panel panel-default">
              <div class="panel-body">
                        <ol class="breadcrumb">
                              <li><a href="#"><i class="fa fa-home fa-lg"></i>&nbsp;&nbsp;&nbsp;Home</a></li>
                              <li class="active">Mobility Search</li>
                            </ol>
                      </div>
            </div>
        </div>
        
        <div class="row">
            <accordion class="app-accordion">
                <accordion-group is-open="status.open">
                    <accordion-heading>
                        <span class="app-accordion-header">Mobility Order Search</span>
			<i class="pull-right glyphicon app-accordion-open-icon" ng-class="{'glyphicon-chevron-down': status.open, 'glyphicon-chevron-right': !status.open}"></i>
                    </accordion-heading>
                    
                    <div style="border-bottom: solid; border-width: 1px; margin-bottom: 20px; color: #9C9C9C;"></div>

             
                    <div class="row" style="padding-left: 15px; padding-right: 15px;">

                        <div class="col-md-4">
                            <form>
                                <div class="form-group">
                                        <div class="canopi-input-title">Project Order Type:</div>
                                        <div class="dropdown">
                                                <select ng-model="poSearch.orderType" ng-options="p.name for p in poOrderTypeList"
                                                    class="dropdown-select form-control"
                                                        title="Please select" data-live-search="false">
                                                </select>
                                        </div>
                                </div>
<!--                                <div class="form-group">
                                    <label for="projectOrderType">Project Order Type</label>
                                    <input class="form-control" id="projectOrderType" placeholder="">
                                </div>-->
                                <div class="form-group">
                                        <div class="canopi-input-title">Project Order Status</div>
                                        <div class="dropdown">
                                                <select ng-model="poSearch.orderType" ng-options="p.name for p in poOrderTypeList"
                                                    class="dropdown-select form-control"
                                                        title="Please select" data-live-search="false">
                                                </select>
                                        </div>
                                </div>
                            </form>
                            <div class="row">
                                <div class="col-md-12">
                                    <button href='javascript:void(0);' class='btn canopi-btn-blue' ng-click="equipmentSearch()">Search</button>
                                    <button href='javascript:void(0);' class='btn canopi-btn-gray'  ng-click="clear()">Clear</button>
                                </div>
                            </div>
                        </div> <!-- col-md-3 -->

                        <div class="col-md-4">
                            <form>
                                <div class="form-group">
                                        <div class="canopi-input-title">Work Order Type:</div>
                                        <div class="dropdown">
                                                <select ng-model="poSearch.orderType" ng-options="p.name for p in poOrderTypeList"
                                                    class="dropdown-select form-control"
                                                        title="Please select" data-live-search="false">
                                                </select>
                                        </div>
                                </div>
                                <div class="form-group">
                                        <div class="canopi-input-title">Project Order Status</div>
                                        <div class="dropdown">
                                                <select ng-model="poSearch.orderType" ng-options="p.name for p in poOrderTypeList"
                                                    class="dropdown-select form-control"
                                                        title="Please select" data-live-search="false">
                                                </select>
                                        </div>
                                </div>
                            </form>
                        </div>

                        <div class="col-md-4">
                                        <div class="row">
                                                <div class="col-md-6 col-sm-6 col-xs-6">
                                                    <div class="form-group">
                                                        <div class="canopi-input-title">Due Date Start:</div>
                                                        <p class="input-group">
                                                            <input type="text" class="form-control"
                                                                   datepicker-popup="{{format}}" ng-model="poSearch.dueStartDate"
                                                                   is-open="datepickers.dueStartDateDatepicker" min-date="minDate"
                                                                   max-date="'2015-12-25'"
                                                                   datepicker-options="dateOptions"
                                                                   date-disabled="disabled(date, mode)"
                                                                   ng-required="true" close-text="Close"/>
                                                          <span class="input-group-btn">
                                                            <button type="button" class="btn btn-default canopi-btn-sharp-corners" ng-click="open($event, 'dueStartDateDatepicker')"><i
                                                                    class="fa fa-calendar"></i></button>
                                                          </span>
                                                        </p>
                                                    </div>
                                                </div>
                                                <div class="col-md-6 col-sm-6 col-xs-6">
                                                    <div class="form-group">
                                                        <div class="canopi-input-title">Due Date End:</div>
                                                        <p class="input-group">
                                                            <input type="text" class="form-control"
                                                                   datepicker-popup="{{format}}" ng-model="poSearch.dueEndDate"
                                                                   is-open="datepickers.dueEndDateDatepicker" min-date="minDate"
                                                                   max-date="'2015-12-25'"
                                                                   datepicker-options="dateOptions"
                                                                   date-disabled="disabled(date, mode)"
                                                                   ng-required="true" close-text="Close"/>
                                                          <span class="input-group-btn">
                                                            <button type="button" class="btn btn-default canopi-btn-sharp-corners" ng-click="open($event, 'dueEndDateDatepicker')"><i
                                                                    class="fa fa-calendar"></i></button>
                                                          </span>
                                                        </p>
                                                    </div>
                                                </div>
                                        </div>
                        </div>

                    </div>
                </accordion-group>
            </accordion>
        </div>
        
        <div class="row">
            
            <div id="grid1" ui-grid="tasks.gridOptions" class="tasksUIGrid"></div>

