<div class="row">
    <div class="col-md-12">

        <!-- Breadcrumb -->
        <div class="row">
            <div class="panel panel-default">
                <div class="panel-body">
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-home fa-lg"></i>&nbsp;&nbsp;&nbsp;Home</a></li>
                        <li class="active">Workorder Templates</li>
                    </ol>
                </div>
            </div>
        </div>

        <!-- Mobility Search Forms/Selections -->
        <div class="row">
            <accordion class="app-accordion">
                <accordion-group is-open="mobilityorders.searchAccordionOpen">

                    <accordion-heading>
                        <span class="app-accordion-header">Mobility Order Search</span>
                        <i class="pull-right glyphicon app-accordion-open-icon" ng-class="{'glyphicon-chevron-up': mobilityorders.searchAccordionOpen, 'glyphicon-chevron-down': !mobilityorders.searchAccordionOpen}"></i>
                    </accordion-heading>

                    <!-- Container Header Underline -->
                    <div style="border-bottom: solid; border-width: 1px; margin-bottom: 20px; color: #9C9C9C; margin-left: 15px; margin-right: 15px;"></div>

                    <!-- Search Forms/Selections -->
                    <div class="row" style="padding-left: 15px; padding-right: 15px;">
                        <div class="col-md-4">
                            <div class="canopi-input-title">Project Order Type:</div>
                            <!--<div ng-dropdown-multiselect="" options="mobilityorders.poType" events="mobilityorders.poTypeSelectionEvents" selected-model="mobilityorders.poTypeCurrentSelection" extra-settings="mobilityorders.poTypeSettings" translation-texts="'asdfasdf'"></div>-->
                            <div isteven-multi-select
                                 input-model="mobilityorders.poTypeMultiselectPickList"
                                 output-model="mobilityorders.poTypeMultiselectPickListOutput"
                                 button-label="icon name"
                                 item-label="icon name maker"
                                 tick-property="ticked"
                                 max-labels="0"
                                 on-item-click="mobilityorders.poTypeSelectionClickItemEvent(data)"
                                 on-select-none="mobilityorders.poTypeSelectionSelectNoneEvent()"
                                 on-select-all="mobilityorders.poTypeSelectionSelectAllEvent()"
                                 on-reset="mobilityorders.poTypeSelectionResetEvent()"
                                 max-height="350px"
                                 >
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="canopi-input-title">Work Order Type:</div>
<!--                            <div ng-dropdown-multiselect="" options="mobilityorders.woType" events="mobilityorders.woTypeSelectionEvents" selected-model="mobilityorders.woTypeCurrentSelection" extra-settings="mobilityorders.woTypeSettings"></div>                                                                                                                     -->
                            <div isteven-multi-select
                                 input-model="mobilityorders.woTypeMultiselectPickList"
                                 output-model="mobilityorders.woTypeMultiselectPickListOutput"
                                 button-label="icon name"
                                 item-label="icon name maker"
                                 tick-property="ticked"
                                 max-labels="0"
                                 on-item-click="mobilityorders.woTypeSelectionClickItemEvent(data)"
                                 on-select-none="mobilityorders.woTypeSelectionSelectNoneEvent()"
                                 on-select-all="mobilityorders.woTypeSelectionSelectAllEvent()"
                                 on-reset="mobilityorders.woTypeSelectionResetEvent()"
                                 max-height="350px"
                                 >
                            </div>
                        </div>
                    </div>

                    <!-- Search/Clear Buttons -->
                    <div class="row" style="padding-left: 15px; padding-right: 15px; padding-top: 5px;">
                        <div class="col-md-12" style="text-align: right;">
                            <button href='#' class='btn canopi-btn-orange'  ng-disabled="mobilityorders.addWoTemplateButtonDisabled" ng-click="mobilityorders.addWOTemplate()">Add WO Template</button>
                            <button href='#' class='btn canopi-btn-blue'  ng-disabled="mobilityorders.searchButtonDisabled" ng-click="mobilityorders.orderSearch()">Search</button>
                            <button href='#' class='btn canopi-btn-gray'  ng-disabled="mobilityorders.clearButtonDisabled" ng-click="mobilityorders.clear()">Clear</button>
                        </div>
                    </div>

                </accordion-group>                           
            </accordion>
        </div> <!-- Mobility Search Forms/Selections -->

        <!-- Mobility Search Results -->
        <div class="row" ng-show="mobilityorders.displayTable">
            <accordion class="app-accordion">
                <accordion-group is-open="mobilityorders.searchResultsOpen">

                    <accordion-heading>
                        <span class="app-accordion-header">Order Result</span>
                        <i class="pull-right glyphicon app-accordion-open-icon" ng-class="{'glyphicon-chevron-up': mobilityorders.searchResultsOpen, 'glyphicon-chevron-down': !mobilityorders.searchResultsOpen}"></i>
                    </accordion-heading>

                    <!-- Container Header Underline -->
                    <div style="border-bottom: solid; border-width: 1px; margin-bottom: 20px; color: #9C9C9C; margin-left: 15px; margin-right: 15px;"></div>
                    <div style="margin-left: 15px; margin-right: 15px;" 
                         cg-busy="{promise:mobilityorders.orderSearchPromiseTable, message:'Loading Your Data', templateUrl:'lib/angular-busy/custom-templates/mycustomtemplate.html'}" 
                         ui-grid="mobilityorders.gridOptionsSearchResults" 
                         ui-grid-selection ui-grid-resize-columns class="tasksSearchResultsUIGrid">                             
                    </div> 
                </accordion-group>                           
            </accordion>
        </div> <!-- Mobility Search Results -->

        <div ng-show="mobilityorders.displayOrderDetails" ng-include="'app/admin-app/mobilityorders/views/mobilityOrderDetail.html'">
        </div>

    </div> <!-- col-md-12 -->

</div> <!-- row -->
