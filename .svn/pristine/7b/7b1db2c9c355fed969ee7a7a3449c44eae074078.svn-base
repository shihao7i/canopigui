angular.module('admin.app', [ 'ngRoute', 'ngResource', 'ui.bootstrap', 
                                 'ui.router', 'ngCookies'
                       ]);
                  




angular.module('admin.app').config(['$stateProvider', '$urlRouterProvider',  function($stateProvider, $urlRouterProvider) {
	
	  //$urlRouterProvider.otherwise('adminguidashboard');
	  //$urlRouterProvider.otherwise('/');
          
	  $stateProvider
            .state('adminmaingui', {
              abstract: true,
	      url: '/adminmain',
	      templateUrl: 'app/admin-app/adminmain.html'
	    })
	    .state('adminmaingui.dashboard', {
	      url: '/dashboard',
	      templateUrl: 'app/admin-app/dashboard/views/dashboard.html',
	      controller : 'AdminDashboardController',
	      data:{
	    	  pageKey: 'DASHBOARD'
	      }
	   
	    })
            .state('adminmaingui.mobilityordersearch', {
	      url: '/mobilityordersearch',
	      templateUrl: 'app/admin-app/tasks/views/mobilityOrderSearch.html',
	      controller : 'MobilityOrderSearchController',
	      data:{
	    	  pageKey: 'MOBILITYORDERSEARCH'
	      }
	   
	    });
	   
	}]);


angular.module('admin.app').run([function () {

    console.log("Admin App Runs!");
		
}]);
