<div class="row">
	<div class="col-md-12 col-lg-12">
	    <!-- BREADCRUMBS - We can setup with Angular $location -->
		<div class="row">
			<div class="panel panel-default">
				<div class="panel-body">
				    <ol class="breadcrumb">
					  <li><a href="#"><i class="fa fa-home fa-lg"></i>&nbsp;&nbsp;&nbsp;Home</a></li>
					  <li class="active">Disconnect INL/CNL PO</li>
					</ol>
				</div>
			</div>
		</div> 
		<!-- End Breadcrumbs -->
		<!-- Start of Panel -->
		<div class="row">
			<div class="panel panel-default">
				<div class="panel-body">
					<span style="color: #ef6f00; font-size: 26px; font-family: Omnes-ATT-II-Light;">Disconnect Project Order</span>
					<div style="border-bottom: solid; border-width: 1px; margin-bottom: 20px; color: #9C9C9C;"></div>
					
					<!-- FORMS -->
					<div class="row canopi-inputfields"  style="padding-left: 20px; padding-right: 20px;">
						<div class="col-lg-4 col-md-4 col-sm-12">
							<!-- Checkbox Directive -->
							<div checkbox id="ortOrder" value="ORT Order" state="isChecked"></div>
						</div>
					</div>
					
					<div class="row canopi-inputfields"  style="padding-left: 20px; padding-right: 20px;">
						<div class="col-lg-4 col-md-4 col-sm-12">
							<div class="form-group">
								<div class="canopi-input-title">Facility ID:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.facilityId"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							 </div>
							<div class="form-group">
								<div class="canopi-input-title">Diversity Indicator:</div>
								<input type="text" class="form-control" ng-model="disconnectINLCNL.diversityIndicator" placeholder="" disabled>
							</div>
							<div class="form-group">
								<div class="canopi-input-title">Related Facility ID:</div>
								<input type="text" class="form-control" ng-model="disconnectINLCNL.relatedFacilityId" placeholder="" disabled>
							</div>
						</div> <!-- end col -->
						<div class="col-lg-4 col-md-4 col-sm-12">
							<div class="form-group">
								<div class="canopi-input-title">Lag Name:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectLAG.lagName"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
							<div class="form-group">
								<div class="canopi-input-title">Customer Code:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.customerCode"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
							<div class="form-group">
								<div class="canopi-input-title">Customer Name:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.customerName"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
							<div class="form-group">
								<div class="canopi-input-title">SVID:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.svid"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
							<div class="form-group">
								<div class="canopi-input-title">BAN:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.ban"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
						</div><!-- end col-->
						<div class="col-lg-4 col-md-4 col-sm-12">
							
							<div class="form-group required">
                                <div class="canopi-input-title">Due Date:</div>                                
                                <p class="input-group">
	                                <input type="text" class="form-control"
	                                       datepicker-popup="{{format}}" ng-model="disconnectINLCNL.dueDate"
	                                       is-open="datepickers.dueDatepicker" min-date="minDate"
	                                       max-date="'2015-12-25'"
	                                       datepicker-options="dateOptions"
	                                       date-disabled="disabled(date, mode)"
	                                       ng-required="true" close-text="Close"/>
	                              <span class="input-group-btn">
	                                <button type="button" class="btn btn-default canopi-btn-sharp-corners" ng-click="open($event, 'dueDatepicker')"><i
	                                        class="fa fa-calendar"></i></button>
	                              </span>
	                            </p>
	                        </div>
                            <div class="form-group">
								<div class="canopi-input-title">OPT ID:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.optId"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
							<div class="form-group">
								<div class="canopi-input-title">CCNA:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.ccna"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
							<div class="form-group">
								<div class="canopi-input-title">ACNA:</div>
                                 <div class="input-group">
                                     <input type="text" class="form-control" ng-model="disconnectINLCNL.acna"  placeholder="Type and Search">
                                       <span class="input-group-btn">
                                         <button class="btn btn-default canopi-btn-sharp-corners" type="button"><i class="fa fa-search"></i></button>
                                       </span>
                                 </div><!-- /input-group -->
							</div>
						</div>	<!-- end col -->				
					</div>
					<!-- SEARCH AND CLEAR BUTTONS -->
	                <div class="row" style="padding-left: 20px; padding-right: 20px;">
						<div class="col-md-12">
							<button class="btn canopi-btn-orange" ng-disabled="picklistNotReady" ng-click="disconnectINLCNL()"><span>Submit</span></button>
							<button class="btn canopi-btn-gray" ng-disabled="picklistNotReady" ng-click="clear()"><span>Clear</span></button>
						</div>
					</div>
				</div>	
			</div>
		</div>
	</div>
</div>
							
							