angular.module('admin.app').controller('ActivitySearchController', 
        [ '$scope', '$log', '$filter', 'activityTypes', 'AdminJsonService',
        function ($scope, $log, $filter, activityTypes, AdminJsonService) {
            
	'use strict';
	
        var vm = this;
        
        $log.debug("activityTypes => " + angular.toJson(activityTypes));

        // sort the rule category values in ascending order
        vm.activityTypes = $filter('orderBy')(activityTypes, 'name');
        
        
}]);


