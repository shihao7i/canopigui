angular.module('admin.service').service('AdminCommonUtilJsonService', ['$q', '$http', '$log', 'AdminGuiConstants', 
                              function ($q, $http,$log, AdminGuiConstants) {
	
	'use strict';
	
    var baseUrl = '/gui-canopi-js/src/app/admin-app/mock/adminmain/';
 
    if (!AdminGuiConstants.MOCK_DATA_FLAG)
    	baseUrl = '/canopigui/app/admin-app/mock/adminmain/';
	
	this.getAdminGuiServerInfo = function () {

            var deferred = $q.defer();
            $http.get(baseUrl + 'serverinfo.json').success(function (response) {
                deferred.resolve(response);
            });
       
            return deferred.promise; 
	};
	

}]);


